{
  "parser": "babel-eslint",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "extends": [
    "airbnb-base", 
    "prettier"
  ],
   "globals": {
    "test": true,
    "window":true,
    "document": true
  },
  "rules": {
      "prettier/prettier": "error",
      "no-console": 1
      // "import/extensions": "never"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
